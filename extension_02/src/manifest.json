{
  "manifest_version": 2,
  "name": "Replaces text 'Google Chrome' with 'Mozilla Firefox' on all sites.",
  "version": "0.2.0",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content/content.js"
      ],
      "css": [
        "content/content.css"
      ]
    }
  ]
}
